<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:color="@color/color_15_ripple_cover"
    tools:ignore="MissingDefaultResource">
    <item>
        <selector>
            <item
                android:drawable="@drawable/nav_icon_main_camera_selected"
                android:state_checked="true"
                android:state_enabled="true" />
            <item
                android:drawable="@drawable/nav_icon_main_camera_default"
                android:state_checked="false"
                android:state_enabled="true" />
            <item
                android:drawable="@drawable/nav_icon_main_camera_disabled"
                android:state_enabled="false" />
        </selector>
    </item>
</ripple>