  xmlns:android="http://schemas.android.com/apk/res/android"
    tools:ignore="MissingDefaultResource">
    <item>
        <selector>
            <item android:state_enabled="true" android:state_checked="true" android:drawable="@drawable/nav_icon_main_camera_selected" />
