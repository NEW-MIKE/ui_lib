<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/main_container"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <com.example.ui_demo_start.view.QMUINotchConsumeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <FrameLayout
            android:id="@+id/frame_container"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" />

        <include
            android:id="@+id/air_setting_layout"
            layout="@layout/air_setting_layout"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            />

        <FrameLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <include
                android:id="@+id/guide_tip_focus"
                layout="@layout/high_tip_for_focus"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:visibility="gone"
                />

            <include
                android:id="@+id/guide_tip_main"
                layout="@layout/high_tip_for_main"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:visibility="gone"
                />

            <include
                android:id="@+id/guide_tip_guide"
                layout="@layout/high_tip_for_guide"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:visibility="gone"
                />
        </FrameLayout>
    </com.example.ui_demo_start.view.QMUINotchConsumeLayout>
</RelativeLayout>